<%- include('../partials/header') %>
  
<table id="list">
    <thead>
      <tr>
        <th>Title</th>
        <th>Release<br>Year</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <% businesses.forEach(function(b) { %>
        <tr>
          <td><%= b.title %></td>
          <td><%= b.releaseYear %></td>
          <td><a href="/businesses/<%= b._id %>">DETAILS</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

<%- include('../partials/footer') %>